#~ msgstr "Telepítés"

#~ msgid "Installed and added to toolbar"
#~ msgstr "Telepítve, és hozzáadva az eszköztárhoz"

#~ msgid "Click here to access %(name)s any time."
#~ msgstr "Kattintson ide, hogy mindig elérje a(z) %(name)s kiegészítőt."

#~ msgid "Ok"
#~ msgstr "Ok"

#~ msgid "Preview %(name)s"
#~ msgstr "%(name)s előnézete"
